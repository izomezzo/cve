package politse.util;

import java.io.Serializable;

import politse.entity.BidOrder;

public class BidOrderEvent implements Serializable {

	private static final long serialVersionUID = 1105514333723441712L;
	/**
	 * 
	 */
	private BidOrder bidOrder;
	
	public BidOrderEvent(BidOrder bidOrder){
		
		this.setBidOrder(bidOrder);
	}

	public BidOrder getBidOrder() {
		return bidOrder;
	}

	public void setBidOrder(BidOrder bidOrder) {
		this.bidOrder = bidOrder;
	}

}
